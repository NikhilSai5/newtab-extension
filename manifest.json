{
    "manifest_version": 3,
    "name": "My New Tab Extension",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "chrome_url_overrides": {
      "newtab": "frontend/public/index.html"
    },

    "action": {
      "default_popup": "popup.html"
      
    }
  }
  